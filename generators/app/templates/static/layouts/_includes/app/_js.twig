{% for scripts in APP.scripts %}
    {% if
        IS_DEV
        or
        IS_PUB and not (scripts.id == 'develop')
    %}
        <!-- {{ scripts.id }} -->

        {% for script in scripts.list %}
            <script src="{{ PATH.js ~ scripts.id ~ '/' ~ script ~ '.js' }}"></script>
        {% endfor %}
    {% endif %}
{% endfor %}

{% for scripts in PAGE.scripts %}
    <!-- {{ PAGE.no ~ ' ' ~ PAGE.id ~ ' ' ~ scripts.id }} -->
    {% for script in scripts.list %}
        <script src="{{ PATH.js ~ scripts.id ~ '/' ~ script ~ '.js' }}"></script>
    {% endfor %}
{% endfor %}

